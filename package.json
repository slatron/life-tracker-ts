{
  "name": "life-tracker-ts",
  "description": "Typescript Demo to convert from plain js/jsx to ts/tsx",
  "homepage": "https://slatron.github.io/life-tracker-ts",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --base=./",
